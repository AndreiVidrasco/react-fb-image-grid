{
  "name": "react-fb-image-grid",
  "version": "0.1.5",
  "description": "A beautifully featured image grid library for React which represents the images just like the facebook grid images with the count for extra as well",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run build:component && npm run build:demo",
    "build:component": "babel src -d lib",
    "build:demo": "react-scripts build",
    "clean": "rm -rf lib build",
    "prepublishOnly": "npm run build",
    "start": "npm run start:demo",
    "start:demo": "react-scripts start",
    "test": "npm run test:unit",
    "test:unit": "react-scripts test",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@mui/material": "^5.15.15",
    "react-icons": "^5.1.0",
    "react-player": "^2.16.0"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "author": "Mohammad Kashif Sulaiman",
  "homepage": "https://github.com/Expertizo/react-fb-image-grid",
  "license": "MIT",
  "repository": "https://github.com/Expertizo/react-fb-image-grid",
  "keywords": [
    "react-facebook-images",
    "react-images",
    "react-grid",
    "react-facebook-images-grid",
    "react-fb-images-grid",
    "react-fb-images",
    "react-beautiful-images"
  ]
}
